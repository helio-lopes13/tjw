<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>PrimeiroServlet</servlet-name>
  	<display-name>PrimeiroServlet</display-name>
  	<description></description>
  	<servlet-class>br.edu.ifce.tjw.PrimeiroServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SegundoServlet</servlet-name>
  	<display-name>SegundoServlet</display-name>
